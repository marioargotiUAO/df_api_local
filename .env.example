# Clave de autenticación para acceder a la API
# ⚠️ Cambiar este valor en el archivo .env real
API_KEY=etl-12345

# Número máximo de filas que la API devolverá en respuestas JSON
MAX_JSON_ROWS=100000

# Carpeta donde están guardados los archivos CSV de entrada
DATA_DIR=data

# Carpeta donde se guardarán resultados, descargas o la base de datos
DB_DIR=db

# Nombre por defecto de los archivos descargados
DEFAULT_DOWNLOAD_NAME=df_etl.csv

# Ruta al archivo SQLite donde se puede guardar el resultado de la ETL
SQLITE_PATH=db/etl.db

# Nombre de la tabla dentro de la base de datos SQLite
SQLITE_TABLE=nhanes_etl
